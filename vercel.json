{
    "version": 2,
    "name": "himalaya-scraper-api",
    "builds": [{
        "src": "api/index.js",
        "use": "@vercel/node"
    }],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/health",
            "dest": "/api/index.js"
        },
        {
            "src": "/up",
            "dest": "/api/index.js"
        },
        {
            "src": "/data.json",
            "dest": "/public/data.json"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        }
    }
}